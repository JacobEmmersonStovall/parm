{
  "version": 1,
  "projects": {
    "react-use-viewport-size": {
      "root": "libs/react/use-viewport-size",
      "sourceRoot": "libs/react/use-viewport-size/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/react/use-viewport-size/tsconfig.lib.json",
              "libs/react/use-viewport-size/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/react/use-viewport-size/**"
            ]
          }
        },
        "build": {
          "builder": "@nrwl/web:bundle",
          "options": {
            "outputPath": "dist/libs/react/use-viewport-size",
            "tsConfig": "libs/react/use-viewport-size/tsconfig.lib.json",
            "project": "libs/react/use-viewport-size/package.json",
            "entryFile": "libs/react/use-viewport-size/src/index.ts",
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/react/use-viewport-size/jest.config.js",
            "tsConfig": "libs/react/use-viewport-size/tsconfig.spec.json"
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/react/use-viewport-size/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/react-use-viewport-size",
            "config": {
              "configFolder": "libs/react/use-viewport-size/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "react-playground": {
      "root": "apps/react/playground",
      "sourceRoot": "apps/react/playground/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/react/playground",
            "index": "apps/react/playground/src/index.html",
            "main": "apps/react/playground/src/main.tsx",
            "polyfills": "apps/react/playground/src/polyfills.ts",
            "tsConfig": "apps/react/playground/tsconfig.app.json",
            "assets": [
              "apps/react/playground/src/favicon.ico",
              "apps/react/playground/src/assets"
            ],
            "styles": [
              "apps/react/playground/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react/playground/src/environments/environment.ts",
                  "with": "apps/react/playground/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react-playground:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "react-playground:build:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/react/playground/tsconfig.app.json",
              "apps/react/playground/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/react/playground/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/react/playground/jest.config.js",
            "tsConfig": "apps/react/playground/tsconfig.spec.json"
          }
        }
      }
    },
    "react-playground-e2e": {
      "root": "apps/react/playground-e2e",
      "sourceRoot": "apps/react/playground-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react/playground-e2e/cypress.json",
            "tsConfig": "apps/react/playground-e2e/tsconfig.e2e.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "react-playground:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/react/playground-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/react/playground-e2e/**"
            ]
          }
        }
      }
    },
    "react-use-viewport-size-e2e": {
      "root": "apps/react-use-viewport-size-e2e",
      "sourceRoot": "apps/react-use-viewport-size-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-use-viewport-size-e2e/cypress.json",
            "tsConfig": "apps/react-use-viewport-size-e2e/tsconfig.e2e.json",
            "devServerTarget": "react-use-viewport-size:storybook"
          },
          "configurations": {
            "production": {
              "devServerTarget": "react-use-viewport-size:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/react-use-viewport-size-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/react-use-viewport-size-e2e/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "tslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "tslint"
      }
    }
  },
  "defaultProject": "react-playground"
}
